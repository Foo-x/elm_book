{
  "title":"Elmの薄い本",

  "plugins": ["japanese-support",
              "snippet@git+https://github.com/cucumber-ltd/gitbook-plugin-snippet.git",
              "numbered-headings",
              "autocover"],

  "pluginsConfig": {
    "snippet": {
      "problem": "fail"
    },
    "autocover": {
            "font": {
                "size": null,
                "family": "Yu Gothic",
                "color": "#FFF"
            },
            "size": {
                "w": 1800,
                "h": 2360
            }
  },
    "structure": {
      "summary": "SUMMARY.md"
  },

  "pdf": {
      "fontFamily": "IPAGothic",      "paperSize" : "b5"
    },

  "styles":{
    "pdf" : "pdf.css"
  }
}
